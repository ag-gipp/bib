@Article{Gipp99,
  Title                    = {{D}er {H}igh-{T}ech {B}ernhardiner - {E}in {R}ettungssystem fuer {L}awinenopfer ("{T}he {H}igh-tech {S}t. {B}ernhard - an avalanche rescue system")},
  Author                   = {{G}ipp, {B}ela and {S}tiller, {J}an-{O}laf},
